{
  "name": "projetoAula",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "cd android && ./gradlew clean && cd .. && npx react-native run-android",
    "ios": "npx react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "commit": "git add . && git commit -m'acertos' && git push",
    "build": "cd android && ./gradlew assembleRelease",
    "clear": "cd android && ./gradlew clean && cd .. && cd ios && pod cache clean --all && rm -rf build && cd .. && rm -rf node_modules && npm cache clean --force && npm install && cd ios && pod install"
  },
  "dependencies": {
    "react": "18.1.0",
    "react-native": "0.70.6",
    "@react-native-async-storage/async-storage": "^1.17.3",
    "@react-native-clipboard/clipboard": "^1.11.1",
    "@react-native-community/checkbox": "^0.5.12",
    "@react-navigation/drawer": "^6.4.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "axios": "^0.27.2",
    "moment": "^2.29.3",
    "react-native-device-info": "^10.0.2",
    "react-native-gesture-handler": "^2.5.0",
    "react-native-input-select": "^0.18.0",
    "react-native-loading-spinner-overlay": "^1.1.0",
    "react-native-masked-text": "^1.13.0",
    "react-native-qrcode-svg": "^6.1.2",
    "react-native-reanimated": "^2.2.4",
    "react-native-safe-area-context": "^4.2.5",
    "react-native-screens": "^3.15.0",
    "react-native-simple-radio-button": "^2.7.4",
    "react-native-svg": "^12.3.0",
    "react-native-uuid": "^2.0.1",
    "react-native-vector-icons": "^9.1.0",
    "react-redux": "^8.0.1",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0"
  },
  "jest": {
    "preset": "react-native"
  }
